{
  "extends": "eslint-config-airbnb",
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, { "ignore": ["electron"] }],
    "linebreak-style": 0,
    "no-unused-vars": 0,
    "no-restricted-syntax": 0,
    "quote-props": 0,
    "max-len": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "object-shorthand": 0,
    "no-param-reassign": 0,
    "no-mixed-operators": 0,
    "no-use-before-define": 0
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  }
}
